<plugin name='rateable' version='0.6.2' grailsVersion='1.1 &gt; *'>
  <author>Matthew Taylor</author>
  <authorEmail>matthew@g2one.com</authorEmail>
  <title>Rateable Plugin</title>
  <description>\
A plugin that adds a generic mechanism for rating domain objects.
</description>
  <documentation>http://grails.org/plugin/rateable</documentation>
  <resources>
    <resource>DataSource</resource>
    <resource>UrlMappings</resource>
    <resource>org.grails.rateable.RateableController</resource>
    <resource>org.grails.rateable.Rating</resource>
    <resource>org.grails.rateable.RatingLink</resource>
    <resource>org.grails.rateable.TestDomain</resource>
    <resource>org.grails.rateable.TestRater</resource>
    <resource>org.grails.rateable.RateableTagLib</resource>
  </resources>
  <dependencies>
    <plugin name='hibernate' version='1.1 &gt; *' />
    <plugin name='yui' version='2.7.0.1 &gt; *' />
    <resolvers />
  </dependencies>
  <behavior>
    <method name='rate' artefact='Domain' type='org.grails.rateable.TestDomain'>
      <description />
      <argument type='java.lang.Object' />
      <argument type='java.lang.Double' />
    </method>
    <method name='getRatings' artefact='Domain' type='org.grails.rateable.TestDomain'>
      <description />
    </method>
    <method name='getAverageRating' artefact='Domain' type='org.grails.rateable.TestDomain'>
      <description />
    </method>
    <method name='getTotalRatings' artefact='Domain' type='org.grails.rateable.TestDomain'>
      <description />
    </method>
    <method name='userRating' artefact='Domain' type='org.grails.rateable.TestDomain'>
      <description />
      <argument type='java.lang.Object' />
    </method>
    <static-method name='listOrderByAverageRating' artefact='Domain' type='org.grails.rateable.TestDomain'>
      <description />
      <argument type='java.util.Map' />
    </static-method>
    <static-method name='countRated' artefact='Domain' type='org.grails.rateable.TestDomain'>
      <description />
    </static-method>
    <property name='ratings' type='org.grails.rateable.TestDomain' artefact='Domain'>
      <description />
    </property>
    <property name='averageRating' type='org.grails.rateable.TestDomain' artefact='Domain'>
      <description />
    </property>
    <property name='totalRatings' type='org.grails.rateable.TestDomain' artefact='Domain'>
      <description />
    </property>
  </behavior>
</plugin>